{
  "compilerOptions": {
    "skipLibCheck": true,
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es2022",
    "noImplicitAny": true,
    "strictNullChecks": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "rootDir": "nodes/",
    "removeComments": true,
  },
  "exclude": [
    "**/*.spec.ts"
  ]
}